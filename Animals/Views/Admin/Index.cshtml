@model IEnumerable<Animals.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        Users
    </div>
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
        @if (Model == null)
        {
            <tr>
                <td colspan="3" class="text-center">Kullanıcı Yok</td>
            </tr>
        }
        else
        {
            foreach (var i in Model)
            {
                <tr>
                    <td>@i.UserID</td>
                    <td>@i.UserName</td>
                    <td>@i.Email</td>
                </tr>
            }

        }
    </table>
</div>